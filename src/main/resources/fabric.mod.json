{
  "schemaVersion": 1,
  "name": "Envy Client",
  "id": "envy",
  "version": "1.0.0",
  "description": "Envy Client is an advanced Minecraft utility client for Fabric that is a continuation of MatHax.",
  "authors": [
    "Volcan",
    "HardlineMouse16"
  ],
  "icon": "assets/mathax/textures/icons/envy.png",
  "license": "GPL-3.0",
  "contact": {
    "homepage": "https://volcanware.xyz/",
    "discord": "https://volcanware.xyz/",
    "issues": "https://volcanware.xyz/"
  },
  "environment": "client",
  "entrypoints": {
    "client": [
      "envy.client.Envy"
    ]
  },
  "mixins": [
    "envy.mixins.json",
    "envy-baritone.mixins.json",
    "envy-canvas.mixins.json",
    "envy-indigo.mixins.json"
  ],
  "accessWidener": "envy.accesswidener",
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.website": "https://volcanware.xyz/",
        "modmenu.discord": "https://volcanware.xyz/",
        "modmenu.issues": "https://volcanware.xyz/"
      }
    }
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=@mc_version@",
    "fabricloader": ">=0.12.12"
  },
  "breaks": {
    "optifabric": "*",
    "feather": "*",
    "origins": "*"
